{
  "private": true,
  "name": "mastodon-flock",
  "version": "1.0.1",
  "description": "Migration assistant from Twitter to Mastodon",
  "homepage": "https://mastodon-flock.vercel.app/",
  "repository": "github:WesSouza/mastodon-flock",
  "author": "Wes Souza <hey@wes.dev>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro build",
    "check": "astro check && tsc --noEmit && pnpm run lint",
    "crawl-instances": "ts-node-esm ./scripts/crawl-instances.ts",
    "dev": "astro dev",
    "lint": "eslint src && prettier --check src",
    "preview": "astro preview",
    "refresh-instances-cache": "ts-node-esm ./scripts/refresh-instances-cache.ts",
    "start": "astro dev"
  },
  "engines": {
    "pnpm": ">=7"
  },
  "packageManager": "pnpm@7.16.1",
  "dependencies": {
    "@astrojs/react": "^2.1.2",
    "@astrojs/vercel": "^3.2.5",
    "@nanostores/react": "^0.5.0",
    "@sentry/browser": "^7.50.0",
    "@sentry/integrations": "^7.50.0",
    "astro": "^2.3.3",
    "cookie": "^0.5.0",
    "date-fns": "^2.30.0",
    "focus-trap-react": "^10.1.1",
    "mongoose": "^7.1.0",
    "nanostores": "^0.8.1",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-window": "^1.8.9",
    "react95": "^4.0.0",
    "styled-components": "^5.3.10",
    "twitter-api-v2": "^1.14.2"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^0.7.2",
    "@types/cookie": "^0.5.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "import-sort-style-wes": "^3.0.2",
    "prettier": "^2.8.8",
    "prettier-plugin-import-sort": "^0.0.7",
    "prop-types": "^15.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "parser": "typescript",
      "style": "wes"
    }
  }
}
